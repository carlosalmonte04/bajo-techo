{
  "name": "bajo-techo",
  "version": "1.0.0",
  "description": "web app for bajo-techo",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --port 3000 --content-base src/ --inline --hot",
    "build:prod": "NODE_ENV=production webpack -p && cp src/index.html dist/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "webpack -p"
  },
  "engines": {
    "node": "6.11.1",
    "npm": "5.5.1"
  },
  "keywords": [
    "bajo-techo"
  ],
  "author": "Carlos Almonte",
  "license": "MIT",
  "dependencies": {
    "dotenv-webpack": "^1.5.4",
    "font-awesome": "^4.7.0",
    "gsap": "^1.20.3",
    "material-ui": "^0.19.4",
    "mdi-material-ui": "^2.0.1",
    "nprogress": "^0.2.0",
    "react": "^16.1.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.1.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "style-loader": "^0.19.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
